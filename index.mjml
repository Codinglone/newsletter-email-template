        <mj-section background-color="#ffffff">
            <mj-group>
                <mj-column>
                    <mj-text align="center" padding="0">
                        <a href="#" class="social_icon-padding"><img src="images/facebook.png" alt="facebook" width="35px" /></a>
                        <a href="#" class="social_icon-padding"><img src="images/twitter.png" alt="twitter" width="35px" /></a>
                        <a href="#"><img src="images/instagram.png" alt="instagram" width="35px" /></a>
                    </mj-text>
                </mj-column>
            </mj-group>
